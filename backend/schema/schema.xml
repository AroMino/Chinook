<?xml version="1.0" encoding="utf-8"?>
<database name="default" defaultIdMethod="native" namespace="\Chinook\Models" defaultPhpNamingMethod="underscore">
  <table name="album" idMethod="native" phpName="Album">
    <column name="album_id" phpName="AlbumId" type="INTEGER" primaryKey="true" autoIncrement="true" required="true"/>
    <column name="title" phpName="Title" type="VARCHAR" size="160" required="true"/>
    <column name="artist_id" phpName="ArtistId" type="INTEGER" required="true"/>
    <foreign-key foreignTable="artist" name="album_artist_id_fkey">
      <reference local="artist_id" foreign="artist_id"/>
    </foreign-key>
    <index name="album_artist_id_idx">
      <index-column name="artist_id"/>
    </index>
  </table>
  <table name="artist" idMethod="native" phpName="Artist">
    <column name="artist_id" phpName="ArtistId" type="INTEGER" primaryKey="true" autoIncrement="true" required="true"/>
