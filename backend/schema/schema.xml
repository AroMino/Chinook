    <column name="last_name" phpName="LastName" type="VARCHAR" size="20" required="true"/>
    <column name="company" phpName="Company" type="VARCHAR" size="80"/>
    <column name="address" phpName="Address" type="VARCHAR" size="70"/>
    <column name="city" phpName="City" type="VARCHAR" size="40"/>
    <column name="state" phpName="State" type="VARCHAR" size="40"/>
    <column name="country" phpName="Country" type="VARCHAR" size="40"/>
    <column name="postal_code" phpName="PostalCode" type="VARCHAR" size="10"/>
    <column name="phone" phpName="Phone" type="VARCHAR" size="24"/>
    <column name="fax" phpName="Fax" type="VARCHAR" size="24"/>
    <column name="email" phpName="Email" type="VARCHAR" size="60" required="true"/>
    <column name="support_rep_id" phpName="SupportRepId" type="INTEGER"/>
    <foreign-key foreignTable="employee" name="customer_support_rep_id_fkey">
      <reference local="support_rep_id" foreign="employee_id"/>
    </foreign-key>
    <index name="customer_support_rep_id_idx">
      <index-column name="support_rep_id"/>
    </index>
